	<style>
		.search {
			margin-bottom: auto;
			margin-top: 70px;
			height: 70px;
			background-color: #fff;
			padding: 10px;
			display: flex;
			align-items: center;
			justify-content: center;
			border-radius: 10px;
		}

		form {
			display: flex;
		}

		button {
			border: none;
		}

		.search-input {

			color: white;
			border: 0;
			outline: 0;
			background: none;
			width: 0;
			margin-top: 5px;
			caret-color: transparent;
			line-height: 40px;
			transition: width 0.4s linear;

		}

		.search .search-input {
			padding: 0 10px;
			width: 550px;
			caret-color: #536bf6;
			font-size: 21px;
			font-weight: 300;
			color: black;
			transition: width 0.4s linear;
		}

		.search:hover>.search-icon {
			background: #207dff;
			color: #fff;
		}

		.search-icon {

			height: 50px;
			width: 50px;
			float: right;
			display: flex;
			justify-content: center;
			align-items: center;
			color: white;
			background-color: #00bd56;

		}

		a:link {
			text-decoration: none;
		}

		.searchresults {
			display: flex;
			width: 80%;
			margin: auto;
			margin-top: 30px;
			align-items: center;
			justify-content: space-around;
			flex-wrap: wrap;
			row-gap: 30px;
			column-gap: 60px;
		}

		.noresults {
			display: flex;
			align-items: center;
			justify-content: center;
		}

		.noresults p {
			font-size: large;
			padding: .7
		}

		.petimage {
			width: 273px;
			height: 269px;
			object-fit: contain;
		}

		.details {
			display: flex;
			flex-direction: column;
			align-items: center;
			justify-content: space-around;
			background: aliceblue;
		}

		.name {
			font-size: 28px;
			margin: 0;
			font-weight: 500;
			color: #846a6a;
		}

		.category {
			margin: 0;
			font-size: 17px;
		}

		.price {
			font-size: 21px;
			margin-top: 15px;
			font-weight: bold;
		}

		.ratingcontainer {
			display: flex;
			align-items: center;
			justify-content: space-between;
			column-gap: 10px;
			margin-bottom: 30px;
		}

		.rating {
			margin: 0;
		}

		.zerorating {
			margin: 0;
			white-space: nowrap;
		}

		.card a {
			box-shadow: rgba(100, 100, 111, 0.2) 0px 7px 29px 0px;
		}

		.card {
			background: aliceblue;
			padding-top: 20px;
		}

		.bestimg {
			width: 209px;
			height: 149px;
			object-fit: contain;
			margin-right: 10px;
		}

		.best-a {
			display: flex;
			align-items: center;
			justify-content: space-around;
		}

		.bestrating {
			font-size: 20px;
			font-weight: bold;
			margin: 0;
		}
	</style>
</head>

<body>

	{{> header home=true}}
	<!-- END nav -->
	<div class="hero-wrap js-fullheight" style="background-image: url('images/bg_1.jpg');"
		data-stellar-background-ratio="0.5">
		<div class="overlay"></div>
		<div class="container">
			<div class="row no-gutters slider-text js-fullheight align-items-center justify-content-center"
				data-scrollax-parent="true">
				<div class="col-md-11 ftco-animate text-center">
					<h1 class="mb-4">Highest Quality Pets At Affordable Rates</h1>
					<div class="d-flex justify-content-center h-100">
						<div class="search">
							<form action="/search" method="post">
								<input type="text" class="search-input" required name="search" placeholder="search..."
									name="">
								<button type="submit"><span  class="search-icon">
										<i class="fa fa-search"></i>
									</span></button>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>


	{{#if searched}}
	<div class="searchresults">

		{{#each data}}
		<div class="card">
			<a href="details?dataID={{this._id}}">
				<div class="image">
					<img class="petimage" src="/petimages/{{this.image1}}" alt="">
				</div>
				<div class="details">
					<p class="name">{{this.name}}</p>
					<p class="category">Category : <span style="font-weight: bold;">{{this.category}}</span></p>
					<p class="price">â‚¹ {{this.price}}</p>

					<div class="ratingcontainer">
						<img src="/images/star.png" alt="">
						{{#if this.rating}}
						<p class="rating">{{this.rating}}</p>
						{{else}}
						<p class="zerorating">&nbsp;&nbsp; 0 Ratings</p>
						{{/if}}
					</div>
				</div>
			</a>
		</div>
		{{/each}}



	</div>
	{{/if}}

	{{#if noresults}}
	<div class="noresults">
		<p>No matches ðŸ˜”</p>
	</div>
	{{/if}}


	<h2 style="text-align: center;margin-top: 60px;">Best sellers</h2>
	<div class="searchresults" style="background: #eeeeee;padding: 2% 5%;width: 100%;">

		{{#each bestsellers}}
		<div class="card" style="padding-top: 0px;padding: 10px;">
			<a href="details?dataID={{this._id}}" class="best-a">
				<div class="image">
					<img class="bestimg" src="/petimages/{{this.image1}}" alt="">
				</div>
				<div class="details">
					<p class="name">{{this.name}}</p>


					<div class="ratingcontainer">
						<img src="/images/star.png" alt="">
						{{#if this.rating}}
						<p class="bestrating">{{this.rating}}</p>
						{{else}}
						<p class="zerorating">&nbsp;&nbsp; 0 Ratings</p>
						{{/if}}
					</div>
				</div>
			</a>
		</div>
		{{/each}}



	</div>

